# このファイルをコピーして terraform.tfvars として使用してください

aws_region = "ap-northeast-1"
app_name   = "oolab-api"
environment = "production"

domain_name = "oolab.tech"
subdomain   = "prod-api.oolab.tech"

# データベース設定
db_instance_class = "db.t3.micro"
db_name          = "oolab_production"
db_username      = "postgres"
db_password      = "your-secure-password-here"

# Rails設定（セキュアな値を設定してください）
rails_master_key = "your-rails-master-key-here"
secret_key_base  = "your-secret-key-base-here"

# CORS設定
cors_origins = "https://oolab.tech,https://www.oolab.tech"